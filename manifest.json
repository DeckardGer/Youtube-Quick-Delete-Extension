{
  "manifest_version": 3,
  "name": "Youtube: Quick Delete",
  "description": "Quickly delete videos from your youtube playlists.",
  "version": "1.0",
  "permissions": ["activeTab"],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["extension/playlistDelete.js", "extension/createInlineScripts.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["web-accessible/autoPlaybackSpeed.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
}
