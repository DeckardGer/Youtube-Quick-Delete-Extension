{
  "manifest_version": 3,
  "name": "Youtube Playlist Quick Delete",
  "description": "Adds a button to Youtubes playlists allowing users to quickly delete their videos",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
